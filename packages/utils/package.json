{
  "name": "@meetgram/utils",
  "version": "0.2.2",
  "license": "MIT",
  "private": false,
  "sideEffects": false,
  "main": "./dist/index.mjs",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.mts",
  "files": [
    "dist/**"
  ],
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "lint:fix": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0 --fix",
    "clean": "rm -rf .turbo && rm -rf node_modules && rm -rf dist"
  },
  "exports": {
    ".": {
      "types": "./dist/index.d.mts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.mjs",
      "default": "./dist/index.mjs"
    },
    "./functions": {
      "types": "./dist/functions/functions.d.ts",
      "import": "./dist/functions/functions.mjs",
      "require": "./dist/functions/functions.mjs",
      "default": "./dist/functions/functions.mjs"
    },
    "./hooks": {
      "types": "./dist/hooks.d.mts",
      "import": "./dist/hooks.mjs",
      "require": "./dist/hooks.mjs",
      "default": "./dist/hooks.mjs"
    }
  },
  "devDependencies": {
    "@it-incubator/eslint-config": "^1.0.1",
    "@it-incubator/prettier-config": "^0.1.2",
    "@meetgram/typescript-config": "workspace:*",
    "@types/react": "^18.2.15",
    "@types/react-dom": "^18.2.7",
    "clsx": "^2.0.0",
    "eslint": "^9.17.0",
    "react": "^18.2.0",
    "react-hook-form": "^7.54.2",
    "tailwind-merge": "^2.5.5",
    "tsup": "^8.3.5",
    "typescript": "^5.2.2"
  },
  "dependencies": {
    "clsx": "^2.0.0",
    "react-intersection-observer": "9.13.0",
    "tailwind-merge": "^2.5.5"
  },
  "peerDependencies": {
    "react": "^16.9.0 || ^17 || ^18",
    "react-dom": "^16.9.0 || ^17 || ^18"
  },
  "publishConfig": {
    "access": "public"
  }
}
